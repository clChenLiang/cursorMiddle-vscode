{
    "name": "cursormiddle",
    "displayName": "cursorMiddle",
    "description": "将当前光标所在的行居中",
    "version": "0.0.1",
    "publisher": "clchenliang",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.saychelinag",
        "onCommand:extension.lineCenter"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "extension.saychelinag",
                "title": "ccc"
            },
            {
                "command": "extension.lineCenter",
                "title": "line to center"
            }
        ],
        "keybindings": [
            {
                "command": "extension.lineCenter",
                "key": "ctrl+;",
                "mac": "enter",
                "when": "editorTextFocus"
            },{
                "command": "extension.lineCenter",
                "key": "ctrl+;",
                "mac": "down",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}